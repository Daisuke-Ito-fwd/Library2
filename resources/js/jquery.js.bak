$("#ajaxUserKey").click(function(){
$.ajax({
    type: "GET",                //接続方法を指定
    url: "/ajax", //接続先ファイル
    dataType: 'json',           //返り値となるファイル形式を指定
    data: {
    'id'   : $('id').val(),
    'name2': $("#name2").val(), //送信する値を指定、いくつでも可。
    'name1': $("#name1").val(), //送信する値を指定、いくつでも可。
    'kana2': $("#kana2").val(), //送信する値を指定、いくつでも可。
    'kana1': $("#kana1").val(), //送信する値を指定、いくつでも可。
    'mail': $("#mail").val(),   //送信する値を指定、いくつでも可。
    'typ': $("#typ").val(),     //送信する値を指定、いくつでも可。
    success:function(data){     //通信に成功した場合
        if(data>=1000){         //処理を記述
        confirm(data+"\n該当データが1000件以上存在します。\n表示に時間がかかる可能性があります。 処理を続けますか？"); 
    }
    $("#srcB").submit();
    },
    error:function(){           //通信に失敗した場合
        return false;
    }
}
});

});